{
  "extends": [
    "next/core-web-vitals"
  ],
  "root": true,
  "rules": {
    "prefer-const": "warn",
    "no-var": "error",
    "react-hooks/exhaustive-deps": "warn",
    "react/display-name": "off",
    "@next/next/no-img-element": "off",
    "react/no-unescaped-entities": "off"
  },
  "ignorePatterns": ["node_modules", ".next", "out"]
}